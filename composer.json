    "require": {},
    "require-dev" : {
        "phpunit/phpunit" : "^7",
        "spatie/phpunit-watcher": "^1.5"
    },
    "autoload" : {
        "psr-4" : {
            "NextSeason\\Model\\SQL\\" : "src/" 
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "NextSeason\\Test\\Model\\" : "tests/"
        }
    },
    "scripts" : {
        "test-watch" : "./vendor/bin/phpunit-watcher watch tests",
        "test-quoter" : "./vendor/bin/phpunit-watcher watch tests/Common/QuoterTest.php",
        "test-expression" : "./vendor/bin/phpunit-watcher watch tests/Common/ExpressionTest.php",
        "test" : "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
    },
    "process-timeout" : 0
}
